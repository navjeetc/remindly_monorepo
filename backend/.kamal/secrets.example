# Kamal secrets file
# Copy this to .kamal/secrets and fill in your actual values
# NEVER commit .kamal/secrets to git!

# DigitalOcean Container Registry token
# Get from: https://cloud.digitalocean.com/account/api/tokens
KAMAL_REGISTRY_PASSWORD=your_do_registry_token_here

# Rails master key for production
# Generate with: EDITOR=nano rails credentials:edit --environment production
# This will create config/credentials/production.key
RAILS_MASTER_KEY=your_production_master_key_here
