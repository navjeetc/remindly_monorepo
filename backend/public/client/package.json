{
  "name": "remindly-web-client",
  "version": "1.0.0",
  "description": "Web client for Remindly with voice announcements using Web Speech API",
  "main": "index.html",
  "scripts": {
    "start": "python3 -m http.server 8080",
    "serve": "npx http-server -p 8080",
    "dev": "npx live-server --port=8080 --open=/index.html"
  },
  "keywords": [
    "reminders",
    "voice",
    "speech-synthesis",
    "web-speech-api",
    "notifications"
  ],
  "author": "Remindly Team",
  "license": "MIT",
  "devDependencies": {
    "http-server": "^14.1.1",
    "live-server": "^1.2.2"
  }
}
